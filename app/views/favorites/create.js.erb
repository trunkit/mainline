(function() {
  var favorited = <%= @status %>;

  if(favorited)
    $(".favorite.item-activity-<%= @activity.id %> img").attr("src", "<%= j(asset_path("feed/favorite-selected.png")) %>");
  else
    $(".favorite.item-activity-<%= @activity.id %> img").attr("src", "<%= j(asset_path("feed/favorite.png")) %>");
})();
